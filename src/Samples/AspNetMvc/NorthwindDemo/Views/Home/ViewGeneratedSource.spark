<viewdata model="Spark.CompiledViewHolder.Entry"/>

#Break(ViewData.Model == null);

<div if="ViewData.Model == null">
  <p>A compiled view class for that of controller and and view is not currently in memory.</p>
</div>
<else>
  <content def="source">${ViewData.Model.Compiler.SourceCode}</content>
  !{source.Replace("\r\n", "<br/>").Replace(" ", "&nbsp;")}
</else>

